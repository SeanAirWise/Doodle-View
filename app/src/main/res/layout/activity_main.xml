<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    <Spinner
        android:id="@+id/spinner4"
        android:layout_width="175dp"
        android:layout_height="wrap_content"

        android:layout_marginStart="435dp"
        android:layout_marginBottom="5dp"
        app:layout_constraintBottom_toBottomOf="@+id/GroundControlInput"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="HardcodedText" />

    <Spinner
        android:id="@+id/spinner5"
        android:layout_width="175dp"
        android:layout_height="wrap_content"

        android:layout_marginStart="435dp"
        android:layout_marginBottom="5dp"
        app:layout_constraintBottom_toBottomOf="@+id/connecteddroneIP"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/readclient3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="175dp"

        android:backgroundTint="#4472C4"
        android:textColor="@color/white"

        android:text="@string/qgroundcontrol_mission_planner_connect_drone_ip_copy_to_clipboard_"

        android:textAllCaps="false"

        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="TextContrastCheck" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="245dp"
        android:layout_marginTop="15dp"
        android:text="Ground Control IP"
        android:textColor="@color/black"
        android:textSize="18sp"
        android:textStyle="bold"

        android:typeface="normal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/readclient3"
        tools:ignore="TextContrastCheck" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="430dp"
        android:text="Radio Frequency"
        android:textColor="@color/black"
        android:textSize="18sp"
        android:textStyle="bold"

        android:typeface="normal"
        app:layout_constraintBottom_toTopOf="@+id/connecteddroneIP"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="TextContrastCheck" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="430dp"
        android:text="Radio Frequency"
        android:textColor="@color/black"
        android:textSize="18sp"
        android:textStyle="bold"

        android:typeface="normal"
        app:layout_constraintBottom_toBottomOf="@+id/textView8"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="TextContrastCheck" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginTop="15dp"
        android:text="@string/ground_control_radio"
        android:textColor="@color/black"
        android:textSize="18sp"
        android:textStyle="bold"

        android:typeface="normal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/readclient3"
        tools:ignore="TextContrastCheck" />

    <EditText
        android:id="@+id/GroundControlInput"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginStart="245dp"
        android:autofillHints=""
        android:hint="@string/_192_168_xxx_xxx"
        android:inputType="textUri"


        android:lines="1"
        android:maxLength="15"
        android:maxLines="1"
        android:minHeight="48dp"
        android:textAppearance="@android:style/TextAppearance.DeviceDefault.Medium"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8"
        tools:ignore="TextContrastCheck,DuplicateSpeakableTextCheck" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="245dp"
        android:layout_marginTop="125dp"
        android:text="Drone IP to Connect"
        android:textColor="@color/black"
        android:textSize="18sp"
        android:textStyle="bold"

        android:typeface="normal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/readclient3"
        tools:ignore="TextContrastCheck" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginTop="125dp"
        android:text="@string/drone_radio_type"
        android:textColor="@color/black"
        android:textSize="18sp"
        android:textStyle="bold"

        android:typeface="normal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/readclient3"
        tools:ignore="TextContrastCheck" />

    <EditText
        android:id="@+id/connecteddroneIP"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginStart="245dp"

        android:autofillHints=""
        android:hint="@string/_192_168_xxx_xxx"
        android:inputType="textUri"
        android:lines="1"
        android:maxLength="15"
        android:maxLines="1"
        android:textAppearance="@android:style/TextAppearance.DeviceDefault.Medium"

        android:textIsSelectable="true"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView10"
        tools:ignore="TextContrastCheck" />

    <Button
        android:id="@+id/nav"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="90dp"
        android:layout_marginBottom="25dp"
        android:text="@string/home"
        android:textAllCaps="false"
        android:backgroundTint="#4472C4"
        android:textColor="@color/white"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="TextContrastCheck,DuplicateSpeakableTextCheck" />

    <Button
        android:id="@+id/nav2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginBottom="25dp"
        android:backgroundTint="#4472C4"
        android:text="@string/gcs_radio_"
        android:textAllCaps="false"
        android:textColor="@color/white"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/nav" />


    <Button
        android:id="@+id/nav3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginBottom="25dp"
        android:backgroundTint="#4472C4"
        android:text="@string/drone_radio_"
        android:textAllCaps="false"
        android:textColor="@color/white"


        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/nav2" />

    <Button
        android:id="@+id/nav4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginBottom="25dp"
        android:text="@string/_help"
        android:textAllCaps="false"
        android:backgroundTint="#4472C4"
        android:textColor="@color/white"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/nav3"
        tools:ignore="TextContrastCheck,DuplicateSpeakableTextCheck" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="200dp"
        android:layout_marginTop="-10dp"
        android:contentDescription="@string/doodleviewlogo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/dv_13" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="600dp"
        android:layout_height="80dp"
        android:layout_marginBottom="9dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/navigation_bar_3"
        android:contentDescription="@string/img11" />

    <Spinner
        android:id="@+id/spinner2"
        android:layout_width="215dp"
        android:layout_height="wrap_content"

        android:layout_marginStart="15dp"
        android:layout_marginBottom="-25dp"
        app:layout_constraintBottom_toBottomOf="@+id/GroundControlInput"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="HardcodedText" />

    <Spinner
        android:id="@+id/spinner3"
        android:layout_width="215dp"
        android:layout_height="wrap_content"

        android:layout_marginStart="15dp"
        android:layout_marginBottom="-25dp"
        app:layout_constraintBottom_toBottomOf="@+id/connecteddroneIP"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="HardcodedText" />

</androidx.constraintlayout.widget.ConstraintLayout>







